{
    "timeout": 3,

    "login": {
        "url": "http://net.tsinghua.edu.cn/do_login.php",
        "parameters": {
            "fixed": {
                "action": "login",
                "ac_id": "1"
            },
            "variable": {
                "username": "username",
                "password": "password"
            }
        },
        "result": {
            "default": false,
            "others": {
                "Login is successful.": true
            }
        }
    },

    "logout": {
        "url": "http://net.tsinghua.edu.cn/do_login.php",
        "parameters": {
            "fixed": {
                "action": "logout"
            },
            "variable": {}
        },
        "result": {
            "default": false,
            "others": {
                "logout_ok": true,
                "not_online_error": true
            }
        }
    },

    "online": {
        "url": "http://net.tsinghua.edu.cn/do_login.php",
        "parameters": {
            "fixed": {
                "action": "check_online"
            },
            "variable": {}
        },
        "result": {
            "default": false,
            "others": {
                "online": true,
                "not_online": false
            }
        }
    },

    "info": {
        "url": "http://net.tsinghua.edu.cn/rad_user_info.php",
        "parameters": {
            "fixed": {},
            "variable": {}
        },
        "result": {}
    },

    "info_parse": {
        "sep": ",",
        "length": 13,
        "username": 0,
        "time_begin": 1,
        "time_end": 2,
        "usage": 6,
        "ip": 8,
        "money_remain": 11
    }
}